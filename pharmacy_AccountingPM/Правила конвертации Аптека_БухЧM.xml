		|	ТаблицаТовары КАК ТаблицаТовары
		|	ЛЕВОЕ СОЕДИНЕНИЕ
		|		Цены КАК Цены
		|		ПО
		|			ТаблицаТовары.Номенклатура = Цены.Номенклатура
		|			И ТаблицаТовары.Серия = Цены.Серия
		|			И ТаблицаТовары.Партия = Цены.Партия
		|			И ТаблицаТовары.ИсточникФинансирования = Цены.ИсточникФинансирования
		|";
		
	КонецЕсли
	
Иначе
	
	ТекстЗапроса = "";
	Для Индекс = 0 По ИменаТаблиц.ВГраница() Цикл
		Если Не ПустаяСтрока(ТекстЗапроса) Тогда
			ТекстЗапроса = ТекстЗапроса + "
			|ОБЪЕДИНИТЬ ВСЕ
			|"
		КонецЕсли;
		ИмяТаблицы = ИменаТаблиц[Индекс];
		ТекстЗапроса = ТекстЗапроса + "
		|ВЫБРАТЬ
		|	" + Строка(Индекс) + " КАК НомерТаблицы,
		|	ТаблицаТовары.Номенклатура КАК Номенклатура,
		|	ТаблицаТовары.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
		|	ТаблицаТовары.Коэффициент КАК Коэффициент,
		|	ТаблицаТовары.КоличествоВЕдиницахИзмерения КАК КоличествоМест,
		|	ТаблицаТовары.Количество КАК Количество,
		|	0 КАК Себестоимость
		|" + ПоляВыборки + "
		|ИЗ
		|	Документ." + МетаданныеДокумента.Имя + ?(ПустаяСтрока(ИмяТаблицы), "", "." + ИмяТаблицы) + " КАК ТаблицаТовары
		|ГДЕ
		|	ТаблицаТовары.Ссылка = &amp;Ссылка
		|";
	КонецЦикла;
	
КонецЕсли;

Запрос = Новый Запрос(ТекстЗапроса);
Запрос.УстановитьПараметр("Ссылка", ДокументСсылка);

ТаблицаТовары = Запрос.Выполнить().Выгрузить();

Если Параметры.ВыгружатьСебестоимостьСписания Или Параметры.СворачиватьНоменклатуруПоГруппеБухУчета Тогда
	
	НайденныеСтроки = ТаблицаТОвары.НайтиСтроки(Новый Структура("Себестоимость", 0));
	Если НайденныеСтроки.Количество() Тогда
		
		УникальныеЗначенияНоменклатуры = Новый Соответствие;
		СписокНоменклатуры = "";
		Для Каждого ТекущаяСтрока Из НайденныеСтроки Цикл
			Если УникальныеЗначенияНоменклатуры.Получить(ТекущаяСтрока.Номенклатура) = Неопределено Тогда
				УникальныеЗначенияНоменклатуры.Вставить(ТекущаяСтрока.Номенклатура, ТекущаяСтрока.Номенклатура);
				СписокНоменклатуры = СписокНоменклатуры + Символы.ПС + Строка(ТекущаяСтрока.Номенклатура);
			КонецЕсли;
		КонецЦикла;
		
		ТекстОшибки = НСтр("ru = 'При выгрузке документа (%1) возникли ошибки.
								 |По номенклатуре нулевая себестоимость:'");
		ТекстОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ТекстОшибки, ДокументСсылка) + СписокНоменклатуры;
		ЗаписатьВПротоколВыполнения(ТекстОшибки,,Истина,,,, Перечисления.РезультатыВыполненияОбмена.Ошибка);
		
	КонецЕсли;
	
КонецЕсли;</Текст>
				<Параметры>ПараметрыПолученияТоваров, ТаблицаТовары</Параметры>
			</Алгоритм>
		</Группа>
		<Группа Имя="АлгоритмыЗагрузкиДокументов">
			<Алгоритм Имя="ЗаполнениеДокументовОтложенное" ИспользуетсяПриЗагрузке="true">
				<Текст>// Параметры
// - Объект
// - ИмяПКО
// - ПараметрыОбъекта

Попытка
	Если		ИмяПКО = "ПоступлениеТоваровУслуг" Тогда

					Выполнить(Алгоритмы.ОбработатьПоступлениеТоваровУслуг);

	ИначеЕсли	ИмяПКО = "ПоступлениеДопРасходов" Тогда

					Выполнить(Алгоритмы.ОбработатьПоступлениеДопРасходов);

	ИначеЕсли	ИмяПКО = "ВозвратТоваровПоставщику" Тогда

					Выполнить(Алгоритмы.ОбработатьВозвратТоваровПоставщику);

	ИначеЕсли	ИмяПКО = "ОтчетОРозничныхПродажах" Тогда

					Выполнить(Алгоритмы.ОбработатьОтчетОРозничныхПродажах);

	ИначеЕсли	ИмяПКО = "ВПТ_ТребованиеНакладная"
				ИЛИ ИмяПКО = "ВПТО_ТребованиеНакладная"
				ИЛИ ИмяПКО = "ПТНВ_ТребованиеНакладная" 
				ИЛИ ИмяПКО = "ОТО_ТребованиеНакладная" Тогда

					Выполнить(Алгоритмы.ОбработатьТребованиеНакладную);

	ИначеЕсли	ИмяПКО = "ПеремещениеТоваров"
				ИЛИ ИмяПКО = "ОТО_ПеремещениеТоваров"
				ИЛИ ИмяПКО = "ВТО_ПеремещениеТоваров" Тогда

					Выполнить(Алгоритмы.ОбработатьПеремещениеТоваров);

	ИначеЕсли	ИмяПКО = "СНТ_СписаниеТоваров"
				ИЛИ ИмяПКО = "СНТО_СписаниеТоваров" Тогда

					Выполнить(Алгоритмы.ОбработатьСписаниеТоваров);

	ИначеЕсли	ИмяПКО = "ОИТ_ОприходованиеТоваров"
				ИЛИ ИмяПКО = "ОИТО_ОприходованиеТоваров" Тогда

					Выполнить(Алгоритмы.ОбработатьОприходованиеТоваров);

	ИначеЕсли	ИмяПКО = "ПОТ_ПоступлениеИзПереработки" Тогда

					Выполнить(Алгоритмы.ОбработатьПоступлениеИзПереработки);

	ИначеЕсли	ИмяПКО = "ПТС_ПередачаТоваров" Тогда

					Выполнить(Алгоритмы.ОбработатьПередачуТоваров);

	ИначеЕсли	ИмяПКО = "ИзготовлениеПоЛекарственнойПрописи"
				ИЛИ ИмяПКО = "Фасовка" Тогда

					Выполнить(Алгоритмы.ОбработатьОтчетПроизводстваЗаСмену);
		
	ИначеЕсли	ИмяПКО = "РеализацияТоваровУслуг" Тогда

					Выполнить(Алгоритмы.ОбработатьРеализацияТоваровУслуг);
					
	КонецЕсли;
Исключение
	ЗарегистрироватьПредупреждение(НСтр("ru = 'Ошибка при выполнении отложенного заполнения. Правила конвертации: '")+ИмяПКО);
КонецПопытки</Текст>
				<Параметры>Объект, ИмяПКО, ПараметрыОбъекта</Параметры>
			</Алгоритм>
			<Алгоритм Имя="ЗаполнениеОтражениеВУСН" ИспользуетсяПриЗагрузке="true">
				<Текст>// Параметры:
// - Объект
// - ТабличнаяЧасть

Если УчетнаяПолитика.Существует(Объект.Организация, Объект.Дата, Истина, Объект.Ссылка) Тогда
	ВариантОтраженияУСН = ?(УчетнаяПолитика.ПрименяетсяУСН(Объект.Организация, Объект.Дата) И
						УчетнаяПолитика.ПрименяетсяУСНДоходыМинусРасходы(Объект.Организация, Объект.Дата),
						Перечисления.ОтражениеВУСН.Принимаются, Перечисления.ОтражениеВУСН.ПустаяСсылка());
	// Заполняем отражение в УСН для ТЧ
	Для Каждого СтрокаТаблицы Из ТабличнаяЧасть Цикл
		Если Не ЗначениеЗаполнено(СтрокаТаблицы.ОтражениеВУСН) Или ВариантОтраженияУСН = Перечисления.ОтражениеВУСН.ПустаяСсылка() Тогда
			СтрокаТаблицы.ОтражениеВУСН = ВариантОтраженияУСН;
		КонецЕсли;
	КонецЦикла;
КонецЕсли;</Текст>
				<Параметры>Объект, ТабличнаяЧасть</Параметры>
