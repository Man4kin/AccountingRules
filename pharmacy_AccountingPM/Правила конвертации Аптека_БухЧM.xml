	<Алгоритмы>
		<Группа Имя="АлгоритмыВыгрузкиДокументов">
			<Алгоритм Имя="ПолучитьТаблицуТоваров" ИспользуетсяПриЗагрузке="false">
				<Текст>
// Параметры
//	- ПараметрыПолученияТоваров - Структура
//		- ДокументСсылка
//		- ИменаТаблиц - Массив
//		- ДополнительныеПоляВыборки - Структура
//	- ТаблицаТовары

ПоляВыборки = "";
Поля = "";
ДополнительныеПоляВыборки = Неопределено;
Если ПараметрыПолученияТоваров.Свойство("ДополнительныеПоляВыборки", ДополнительныеПоляВыборки) Тогда
	Для Каждого КлючЗначение Из ДополнительныеПоляВыборки Цикл
		ПоляВыборки = ПоляВыборки + ", " + Символы.ПС + ?(ЗначениеЗаполнено(КлючЗначение.Значение), КлючЗначение.Значение, КлючЗначение.Ключ)  + " КАК " + КлючЗначение.Ключ;
		Поля = Поля + ", " + Символы.ПС + КлючЗначение.Ключ  + " КАК " + КлючЗначение.Ключ;
	КонецЦикла;
КонецЕсли;

ИменаТаблиц = Неопределено;
Если Не ПараметрыПолученияТоваров.Свойство("ИменаТаблиц", ИменаТаблиц) Тогда
	ИменаТаблиц = Новый Массив;
	ИменаТаблиц.Добавить("Товары");
КонецЕсли;

ДокументСсылка = ПараметрыПолученияТоваров.ДокументСсылка;
МетаданныеДокумента = ДокументСсылка.Метаданные();
Если Параметры.ВыгружатьСебестоимостьСписания Или Параметры.СворачиватьНоменклатуруПоГруппеБухУчета Тогда
	
	ИмяРеквизитаСклад = "Склад";
	Если МетаданныеДокумента.Реквизиты.Найти("СкладОтправитель") &lt;&gt; Неопределено Тогда
		ИмяРеквизитаСклад = "СкладОтправитель";
	КонецЕсли;
	
	ТекстЗапроса = "
	|ВЫБРАТЬ
	|	Шапка.Дата КАК ДатаДокумента,
	|	Шапка.Организация КАК Организация,
	|	Шапка." + ИмяРеквизитаСклад + " КАК Склад,
	|	ВЫБОР
	|		КОГДА Шапка." + ИмяРеквизитаСклад + ".ТипСклада = ЗНАЧЕНИЕ(Перечисление.ТипыСкладов.Отделение)
	|			ТОГДА ИСТИНА
	|		ИНАЧЕ ЛОЖЬ
	|	КОНЕЦ КАК УчетВОтделениях,
	|	ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка) КАК ПодразделениеОрганизации,
	|	Шапка.Ссылка КАК Документ, 
	|	ВестиУчетПоИсточникамФинансирования.Значение КАК ВестиУчетПоИсточникамФинансирования
	|ПОМЕСТИТЬ Шапка
	|ИЗ
	|	Документ." + МетаданныеДокумента.Имя + " КАК Шапка,
	|	Константа.ИспользоватьИсточникиФинансирования КАК ВестиУчетПоИсточникамФинансирования
	|ГДЕ
	|	Шапка.Ссылка = &amp;Ссылка
	|;
	|
	|//////////////////////////////////////////////////////////////////////////////
	|";
	
	ТекстЗапросаТоваров = "";
	Для Индекс = 0 По ИменаТаблиц.ВГраница() Цикл
		Если Не ПустаяСтрока(ТекстЗапросаТоваров) Тогда
			ТекстЗапросаТоваров = ТекстЗапросаТоваров + "
			|ОБЪЕДИНИТЬ ВСЕ
			|"
		КонецЕсли;
		ИмяТаблицы = ИменаТаблиц[Индекс];
		ТекстЗапросаТоваров = ТекстЗапросаТоваров + "
		|ВЫБРАТЬ
		|	" + Индекс + " КАК НомерТаблицы,
		|	Шапка.ДатаДокумента КАК ДатаДокумента,
		|	Шапка.Организация КАК Организация,
		|	Шапка.Склад КАК Склад,
		|	Шапка.ПодразделениеОрганизации КАК ПодразделениеОрганизации,
		|	ВЫБОР
		|		КОГДА Шапка.УчетВОтделениях
		|			ТОГДА ЗНАЧЕНИЕ(Перечисление.РазделыУчетаСебестоимостиТоваров.ТоварыВОтделениях)
		|		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.РазделыУчетаСебестоимостиТоваров.ТоварыНаСкладах)
		|	КОНЕЦ КАК РазделУчета,
		|	Шапка.Документ КАК Документ,
		|	ТаблицаТовары.Номенклатура КАК Номенклатура,
		|	ТаблицаТовары.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
		|	ТаблицаТовары.Коэффициент КАК Коэффициент,
		|	ТаблицаТовары.КоличествоВЕдиницахИзмерения КАК КоличествоМест,
		|	ТаблицаТовары.Количество КАК Количество,
		|	ВЫБОР
		|		КОГДА НЕ Шапка.УчетВОтделениях И ТаблицаТовары.Номенклатура.ВестиУчетПоСериям И ТаблицаТовары.Номенклатура.ВестиСуммовойУчетПоСериям
		|			ТОГДА ТаблицаТовары.СерияНоменклатуры
		|		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка)
		|	КОНЕЦ КАК Серия,
		|	ВЫБОР
		|		КОГДА НЕ Шапка.УчетВОтделениях
		|				И ТаблицаТовары.Номенклатура.ВестиУчетПоДокументамОприходования
		|				И ТаблицаТовары.Номенклатура.ВестиСуммовойУчетПоДокументамОприходования
		|			ТОГДА ТаблицаТовары.Партия
		|		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.ПартииНоменклатуры.ПустаяСсылка)
		|	КОНЕЦ КАК Партия,
		|	ВЫБОР
		|		КОГДА Шапка.ВестиУчетПоИсточникамФинансирования
		|			ТОГДА ТаблицаТовары.ИсточникФинансирования
		|		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.ИсточникиФинансирования.ПустаяСсылка)
		|	КОНЕЦ КАК ИсточникФинансирования
		|" + ПоляВыборки + "
		|" + ?(ПустаяСтрока(ТекстЗапросаТоваров), "ПОМЕСТИТЬ ТаблицаТовары", "") + "
		|ИЗ
		|	Документ." + МетаданныеДокумента.Имя + ?(ПустаяСтрока(ИмяТаблицы), "", "." + ИмяТаблицы) + " КАК ТаблицаТовары,
		|	Шапка КАК Шапка
		|ГДЕ
		|	ТаблицаТовары.Ссылка = &amp;Ссылка";
	КонецЦикла;
	ТекстЗапроса = ТекстЗапроса + ТекстЗапросаТоваров +"
	|;
	|
	|//////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ТаблицаТовары.ДатаДокумента КАК ДатаДокумента,
	|	ТаблицаТовары.Организация КАК Организация,
	|	ТаблицаТовары.Склад КАК Склад,
	|	ТаблицаТовары.ПодразделениеОрганизации КАК ПодразделениеОрганизации,
	|	ТаблицаТовары.РазделУчета КАК РазделУчета,
	|	ТаблицаТовары.Документ КАК Документ,
	|	ТаблицаТовары.Номенклатура КАК Номенклатура,
	|	ТаблицаТовары.Серия КАК Серия,
	|	ТаблицаТовары.Партия КАК Партия,
	|	ТаблицаТовары.ИсточникФинансирования КАК ИсточникФинансирования
	|ПОМЕСТИТЬ втТовары
	|ИЗ
	|	ТаблицаТовары КАК ТаблицаТовары
	|;
	|
	|//////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Товары.Номенклатура КАК Номенклатура,
	|	Товары.Серия КАК Серия,
	|	Товары.Партия КАК Партия,
	|	Товары.ИсточникФинансирования КАК ИсточникФинансирования,
	|	Товары.Документ КАК Документ,
	|	ВЫБОР
	|		КОГДА ЕстьNULL(СУММА(СебестоимостьТоваров.Количество), 0) = 0
	|			ТОГДА 0
	|		ИНАЧЕ ВЫРАЗИТЬ(СУММА(СебестоимостьТоваров.Стоимость + СебестоимостьТоваров.СуммаДопРасходов) / СУММА(СебестоимостьТоваров.Количество) КАК ЧИСЛО(20, 6))
	|	КОНЕЦ КАК ЦенаПоСебестоимости
	|ПОМЕСТИТЬ Цены
	|ИЗ
	|	втТовары КАК Товары
	|		ЛЕВОЕ СОЕДИНЕНИЕ
	|			РегистрСведений.АналитикаВидаУчета КАК АналитикиВидаУчета
	|			ПО 
	|				Товары.Организация = АналитикиВидаУчета.Организация
	|				И Товары.Склад = АналитикиВидаУчета.Склад
	|				И АналитикиВидаУчета.ПодразделениеОрганизации = Товары.ПодразделениеОрганизации
	|				И Товары.ИсточникФинансирования = АналитикиВидаУчета.ИсточникФинансирования
	|		ЛЕВОЕ СОЕДИНЕНИЕ
	|			РегистрСведений.АналитикаУчетаНоменклатуры КАК АналитикиУчетаНоменклатуры
	|			ПО 
	|				Товары.Номенклатура = АналитикиУчетаНоменклатуры.Номенклатура
	|				И Товары.Серия = АналитикиУчетаНоменклатуры.СерияНоменклатуры
	|				И Товары.Партия = АналитикиУчетаНоменклатуры.Партия
	|		ЛЕВОЕ СОЕДИНЕНИЕ 
	|			РегистрНакопления.СебестоимостьТоваров КАК СебестоимостьТоваров
	|			ПО 
	|				СебестоимостьТоваров.РазделУчета = Товары.РазделУчета
	|				И АналитикиВидаУчета.КлючАналитики = СебестоимостьТоваров.АналитикаВидаУчета
	|				И АналитикиУчетаНоменклатуры.КлючАналитики = СебестоимостьТоваров.АналитикаУчетаНоменклатуры
	|				И Товары.Документ = СебестоимостьТоваров.Регистратор
	|
	|СГРУППИРОВАТЬ ПО
	|	Товары.Номенклатура,
	|	Товары.Серия,
	|	Товары.Партия,
	|	Товары.Документ,
	|	Товары.ИсточникФинансирования
	|";
	
	Если Параметры.СворачиватьНоменклатуруПоГруппеБухУчета Тогда
		
		ТекстЗапроса = ТекстЗапроса + "
		|;
		|
		|//////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ТаблицаТовары.НомерТаблицы КАК НомерТаблицы,
		|	ТаблицаТовары.Номенклатура.ГруппаБухгалтерскогоУчета КАК Номенклатура,
		|	ЗНАЧЕНИЕ(Справочник.ЕдиницыИзмеренияНоменклатуры.ПустаяСсылка) КАК ЕдиницаИзмерения,
		|	0 КАК Коэффициент,
